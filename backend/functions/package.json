{
  "name": "viki-functions",
  "version": "0.1.0",
  "private": true,
  "description": "Azure Functions app ingesting NetSfere consults and routing them to specialists.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts",
    "prestart": "npm run build",
    "start": "concurrently \"azurite --silent --location ../../ --debug ../../debug.log\" \"func start\"",
    "dev": "npm run build -- --watch"
  },
  "dependencies": {
    "@azure/ai-voicelive": "^1.0.0-beta.2",
    "@azure/functions": "4.4.0",
    "@azure/identity": "^4.13.0",
    "@azure/storage-blob": "12.18.0",
    "axios": "1.6.7",
    "node-record-lpcm16": "^1.0.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "azure-functions-core-tools": "4.6.0",
    "azurite": "3.35.0",
    "eslint": "8.56.0",
    "rimraf": "5.0.5",
    "typescript": "5.4.3"
  }
}
